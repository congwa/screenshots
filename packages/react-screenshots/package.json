{
  "name": "react-screenshots-mind",
  "version": "0.0.1",
  "description": "a picture clipping and graffiti tool by react",
  "main": "dist/screenshots.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "npm run dev:web & npm run dev:electron",
    "dev:web": "webpack-dev-server --config build/webpack.dev.web.js",
    "dev:electron": "webpack --config build/webpack.dev.electron.js",
    "build": "npm run build:web & npm run build:electron",
    "build:web": "webpack --config build/webpack.prod.web.js",
    "build:electron": "webpack --config build/webpack.prod.electron.js",
    "lint": "eslint . --ext .js,.jsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/congwa/screenshots.git"
  },
  "keywords": [
    "clipping",
    "graffiti",
    "react",
    "image"
  ],
  "author": "cong_wa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/congwa/screenshots/issues"
  },
  "homepage": "https://github.com/congwa/screenshots/tree/master/packages/react-screenshots#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "normalize.css": "^8.0.1",
    "raf": "^3.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.1",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
